tab1:CreateWarning("2 - get the auto weapon menu below")
tab1:CreateWarning("3 - then enter the name of the weapon")
tab1:CreateWarning("Exemplo abaixo")
tab1:CreateWarning("Pistol, Machine Gun, Uzi, Trench Shotgun")
tab1:CreateWarning("And Press Active")

